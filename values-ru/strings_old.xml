    <string name="startup_boot_center" formatted="false">"
 *
 *
 *
 *
 JMP 0x1F
 POPL %ESI
 MOVL %ESI,0x8(%ESI)
 XORL %EAX,%EAX
 MOVB %EAX,0x7(%ESI)
 MOVL %EAX,0xC(%ESI)
 MOVB $0xB,%AL
 MOVL %ESI,%EBX
